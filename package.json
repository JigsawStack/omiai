{
  "name": "omniai",
  "description": "OmniAI - A highly opinionated AI SDK with auto-model selection, built-in reasoning and curated tools",
  "homepage": "https://github.com/JigsawStack/omniai",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JigsawStack/omniai.git"
  },
  "bugs": {
    "url": "https://github.com/JigsawStack/omniai/issues"
  },
  "keywords": ["framework", "ai", "llm", "react"],
  "type": "module",
  "version": "0.0.1",
  "packageManager": "yarn@1.22.22",
  "license": "Apache License",
  "engines": {
    "node": ">=18"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "files": ["dist/**"],
  "exports": {
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    },
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build": "pkgroll --src=./",
    "format": "biome check --write ."
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.1.2",
    "@ai-sdk/deepinfra": "^0.1.5",
    "@ai-sdk/deepseek": "^0.1.4",
    "@ai-sdk/google": "^1.1.2",
    "@ai-sdk/groq": "^1.1.2",
    "@ai-sdk/openai": "^1.1.2",
    "ai": "^4.1.5",
    "ai-fallback": "^0.1.1",
    "jigsawstack": "^0.0.23",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.10",
    "dotenv": "^16.4.7",
    "pkgroll": "^2.6.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}
